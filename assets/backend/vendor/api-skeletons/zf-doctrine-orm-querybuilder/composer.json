{
    "name": "api-skeletons/zf-doctrine-orm-querybuilder",
    "type": "library",
    "description": "ORM QueryBuilder filters for Doctrine",
    "keywords": [
        "api",
        "apigility",
        "doctrine",
        "filter",
        "module",
        "zend",
        "zf2"
    ],
    "homepage": "https://github.com/API-Skeletons/zf-doctrine-orm-querybuilder",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Tom Anderson",
            "email": "tom.h.anderson@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "doctrine/doctrine-module": "*",
        "doctrine/doctrine-orm-module": "*",
        "zendframework/zend-modulemanager": "^2.4",
        "zendframework/zend-mvc": "^2.4",
        "zendframework/zend-servicemanager": "^2.4",
        "zfcampus/zf-api-problem": "^1.1",
        "zfcampus/zf-hal": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.1",
        "zendframework/zendframework": "^2.4",
        "zfcampus/zf-apigility-provider": "^1.1",
        "squizlabs/php_codesniffer": "^2.0"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "ZF\\Doctrine\\ORM\\QueryBuilder\\": "src/"
        },
        "classmap": [
          "Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ZFTest\\Doctrine\\ORM\\QueryBuilder\\": "test/"
        }
    }
}
